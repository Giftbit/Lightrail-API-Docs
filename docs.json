{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Willow Starter Kit",
  "logo": "/images/lr-logomark.svg",
  "colors": {
    "primary": "#006AD4",
    "light": "#007FFF",
    "dark": "#006AD4"
  },
  "favicon": "/images/lr-logomark.svg",
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "navigation": {
    "versions": [
      {
        "version": "Public API",
        "default": true,
        "tabs": [
          {
            "tab": "Get Started",
            "groups": [
              {
                "group": "Get Started",
                "pages": [
                  "public-api/welcome",
                  "public-api/quickstart",
                  "public-api/development"
                ]
              },
              {
                "group": "Essentials",
                "pages": [
                  "public-api/essentials/markdown",
                  "public-api/essentials/code",
                  "public-api/essentials/images",
                  "public-api/essentials/settings",
                  "public-api/essentials/navigation",
                  "public-api/essentials/reusable-snippets",
                  "api-reference/create.mdx"
                ]
              }
            ]
          },
          {
            "tab": "API Reference",
            "groups": [
              {
                "group": "API Reference",
                "openapi": "https://assets.lightrail.com/openapi/latest-public.yaml"
              }
            ]
          }
        ]
      },
      {
        "version": "Full API",
        "tabs": [
          {
            "tab": "Get Started",
            "groups": [
              {
                "group": "Get Started",
                "pages": [
                  "full-api/welcome",
                  "full-api/quickstart",
                  "full-api/development"
                ]
              },
              {
                "group": "Essentials",
                "pages": [
                  "full-api/essentials/markdown",
                  "full-api/essentials/code",
                  "full-api/essentials/images",
                  "full-api/essentials/settings",
                  "full-api/essentials/navigation",
                  "full-api/essentials/reusable-snippets",
                  "api-reference/create.mdx"
                ]
              }
            ]
          },
          {
            "tab": "API Reference",
            "groups": [
              {
                "group": "API Reference",
                "openapi": "https://assets.lightrail.com/openapi/latest.yaml"
              }
            ]
          }
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/giftbit"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  }
}
